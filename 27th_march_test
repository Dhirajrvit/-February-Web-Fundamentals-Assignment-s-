var library = [ 
    {
        author: 'Bill Gates',
        title: 'The Road Ahead',
        readingStatus: true

    },
    
    {
        author: 'Steve Jobs',
        title: 'Walter Isaacson',
        readingStatus: true
    },
    {
        author: 'Suzanne Collins',
        title:  'Mockingjay: The Final Book of The Hunger Games', 
        readingStatus: false
    }
    
];
for(var x in library){
    bookTitle = library[x].title
    auth = library[x].author
    Status = library[x].readingStatus
    console.log(bookTitle,auth,Status)
}
